@{
}
<style>
    label{
        font-size: 25px;
    }
</style>
@model IEnumerable<Store_Project.Models.Category>
<div class="aside">
    <h2 class="aside-title">DANH MỤC SẢN PHẨM</h2>
    <div class="checkbox-filter">
        @foreach (var item in Model)
        {
            <div class="input-checkbox">
                <input type="checkbox" id="category-1" />
                <label for="category-1">
                    @item.Name
                </label>
           </div>
        }
    </div>
</div>
